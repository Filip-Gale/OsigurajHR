<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>https://www.osiguraj.hr/</loc>
    <priority>1.0</priority>
  </url>

  {% for stranica in site.tehnicki_pregled %}
  <url>
    <loc>https://www.osiguraj.hr{{ stranica.url }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.8</priority>
  </url>
  {% endfor %}

  {% for g in site.gradovi %}
  <url>
    <loc>https://www.osiguraj.hr{{ g.url }}</loc>
    <priority>0.5</priority>
  </url>
  {% endfor %}

  {% for post in site.posts %}
  <url>
    <loc>https://www.osiguraj.hr{{ post.url }}</loc>
    <priority>0.6</priority>
  </url>
  {% endfor %}
</urlset>
