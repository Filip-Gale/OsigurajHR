---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url><loc>https://www.osiguraj.hr/</loc><priority>1.0</priority></url>
  <url><loc>https://www.osiguraj.hr/kasko/</loc><priority>0.9</priority></url>
  <url><loc>https://www.osiguraj.hr/kontakt/</loc><priority>0.5</priority></url>
  <url><loc>https://www.osiguraj.hr/izracunaj-cijenu/</loc><priority>0.9</priority></url>
  <url><loc>https://www.osiguraj.hr/digitalno-auto-osiguranje/</loc><priority>0.8</priority></url>

  {% for page in site.pages %}
    {% if page.path contains 'registracija/' and page.path contains '.md' %}
    <url>
      <loc>https://www.osiguraj.hr{{ page.url }}</loc>
      <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
      <priority>0.9</priority>
    </url>
    {% endif %}
  {% endfor %}

  {% for grad in site.tehnicki_pregled %}
  <url>
    <loc>https://www.osiguraj.hr{{ grad.url }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.9</priority>
  </url>
  {% endfor %}

  {% for page in site.pages %}
    {% if page.path contains 'auto-osiguranje/' and page.path contains '.md' %}
    <url>
      <loc>https://www.osiguraj.hr{{ page.url }}</loc>
      <priority>0.8</priority>
    </url>
    {% endif %}
  {% endfor %}

  {% for post in site.posts %}
  <url>
    <loc>https://www.osiguraj.hr{{ post.url }}</loc>
    <priority>0.7</priority>
  </url>
  {% endfor %}
</urlset>
