<script>
function izracunajDS() {
    const kw = parseFloat(document.getElementById('kw-input-ds').value) || 0;
    const fiksniTroskovi = 101.35; 
    const varijabilniDio = kw * 0.17; 
    const ukupno = fiksniTroskovi + varijabilniDio;
    
    const prikaz = document.getElementById('rezultat-ds');
    document.getElementById('cifra-ds').innerText = ukupno.toFixed(2) + " â‚¬";
    prikaz.style.display = "block";

    setTimeout(() => {
        prikaz.scrollIntoView({ behavior: 'smooth', block: 'center' });
    }, 100);
}
</script>
